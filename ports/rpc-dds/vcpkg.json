{
  "name": "rpc-dds",
  "version": "0.3.1",
  "description": "eProsima library for RPC over DDS, TCP or REST",
  "homepage": "https://github.com/eProsima/RPC",
  "dependencies": [
    "boost-threadpool",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "fast-dds": {
      "description": "Enable fast-dds from skrede/vcpkg.",
      "dependencies": [
        "fast-dds"
      ]
    },
    "fastrtps": {
      "description": "Enable fastrtps from microsoft/vcpkg.",
      "dependencies": [
        "fastrtps"
      ]
    }
  }
}